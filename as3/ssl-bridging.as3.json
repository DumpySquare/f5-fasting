{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-appsvcs-extension/master/schema/latest/as3-schema.json",
    "class": "AS3",
    "declaration": {
        "class": "ADC",
        "schemaVersion": "3.25.0",
        "id": "urn:uuid:d0afe611-1e7a-4f0b-8fa4-6ba67b1e40c1",
        "label": "ssl-bridging conversion",
        "remark": "Auto-generated by Project Charon",
        "ssl-brdge": {
            "class": "Tenant",
            "main": {
                "class": "Application",
                "template": "generic",
                "ssl_bridge_443_vs": {
                    "class": "Service_HTTPS",
                    "remark": "app note",
                    "persistenceMethods": [
                        {
                            "bigip": "/Common/cookie"
                        }
                    ],
                    "fallbackPersistenceMethod": 
                        {
                            "bigip": "/Common/source_addr"
                        },
                    "profileTCP": {
                        "bigip": "/Common/f5-tcp-progressive"
                    },
                    "layer4": "tcp",
                    "profileAnalyticsTcp": {
                        "use": "ssl_bridge_tcp_analytics"
                    },
                    "profileAnalytics": {
                        "use": "ssl_bridge_analytics"
                    },
                    "pool": "ssl_bridge_443_pool",
                    "translateServerAddress": true,
                    "translateServerPort": true,
                    "virtualAddresses": [
                        "172.24.82.67"
                    ],
                    "virtualPort": 443,
                    "snat": {
                        "use": "ssl_bridge_snatpool"
                    },
                    "serverTLS": "ssl_bridge_client_ssl_profile",
                    "clientTLS": {
                        "bigip": "/Common/serverssl"
                    }
                },
                "ssl_bridge_443_pool": {
                    "remark": "asdf",
                    "members": [
                        {
                            "addressDiscovery": "static",
                            "servicePort": 443,
                            "serverAddresses": [
                                "172.24.171.189"
                            ]
                        }
                    ],
                    "monitors": [
                        {
                            "use": "ssl_bridge_https_monitor"
                        }
                    ],
                    "class": "Pool"
                },
                "ssl_bridge_client_ssl_profile": {
                    "class": "TLS_Server",
                    "label": "simplest decl requires just cert",
                    "certificates": [
                        {
                            "certificate": "ssl_bridge_cert"
                        }
                    ]
                },
                "ssl_bridge_cert": {
                    "class": "Certificate",
                    "certificate": {
                        "bigip": "/Common/default.crt"
                    },
                    "privateKey": {
                        "bigip": "/Common/default.key"
                    }
                },
                "ssl_bridge_https_monitor": {
                    "adaptive": false,
                    "interval": 5,
                    "dscp": 0,
                    "receive": "200 OK",
                    "send": "GET / HTTP/1.1\\r\\nHost: ssl_bridge.com\\r\\nConnection: Close\\r\\n\\r\\n",
                    "timeUntilUp": 0,
                    "timeout": 16,
                    "class": "Monitor",
                    "monitorType": "https",
                    "targetAddress": "",
                    "targetPort": 0,
                    "upInterval": 0,
                    "transparent": false,
                    "reverse": false
                },
                "ssl_bridge_snatpool": {
                    "snatAddresses": [
                        "172.24.82.67"
                    ],
                    "class": "SNAT_Pool"
                },
                "ssl_bridge_analytics": {
                    "capturedTrafficExternalLogging": false,
                    "capturedTrafficInternalLogging": true,
                    "collectGeo": true,
                    "collectClientSideStatistics": true,
                    "collectIp": true,
                    "collectMaxTpsAndThroughput": true,
                    "collectMethod": true,
                    "collectOsAndBrowser": true,
                    "collectPageLoadTime": true,
                    "collectResponseCode": true,
                    "collectSubnet": true,
                    "collectUrl": true,
                    "collectUserAgent": true,
                    "collectUserSession": true,
                    "collectedStatsExternalLogging": false,
                    "collectedStatsInternalLogging": true,
                    "remark": "none",
                    "notificationByEmail": false,
                    "notificationBySnmp": false,
                    "notificationBySyslog": false,
                    "publishIruleStatistics": false,
                    "sessionCookieSecurity": "ssl-only",
                    "sessionTimeoutMinutes": 5,
                    "class": "Analytics_Profile",
                    "captureFilter": {}
                },
                "ssl_bridge_tcp_analytics": {
                    "collectCity": true,
                    "collectContinent": false,
                    "collectCountry": true,
                    "collectNexthop": true,
                    "collectPostCode": true,
                    "collectRegion": true,
                    "collectRemoteHostIp": true,
                    "collectRemoteHostSubnet": true,
                    "collectedByClientSide": true,
                    "collectedByServerSide": true,
                    "collectedStatsExternalLogging": false,
                    "collectedStatsInternalLogging": true,
                    "remark": "none",
                    "class": "Analytics_TCP_Profile"
                }
            }
        }
    }
}